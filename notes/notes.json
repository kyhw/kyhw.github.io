---
layout: none
permalink: /notes/notes.json
---

[
  {% for note in site.notes %}
    {
      "title": "{{ note.title | escape }}",
      "date": "{{ note.date | date: "%B %d, %Y" }}",
      "excerpt": "{{ note.excerpt | default: note.content | strip_html | truncatewords: 20 | escape }}",
      "filename": "{{ note.url | replace: '/notes', '' }}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]