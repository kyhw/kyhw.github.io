---
layout: null
permalink: /notes.json
---
[
  {% for note in site.notes %}
    {
      "title": "{{ note.title | escape }}",
      "date": "{{ note.date | date: "%Y-%m-%d" }}",
      "url": "{{ note.url | relative_url }}",
      "content": {{ note.content | strip_html | jsonify }}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]